{
  "name": "linkens.eth",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf build/*",
    "clean:dapp": "rm -rf build/dapp/*",
    "clean:user": "rm -rf build/user/*",
    "build": "npm run clean && concurrently \"npm:build:*\"",
    "build:dapp": "NODE_ENV=production webpack --env.STAGING=false",
    "build:user": "NODE_ENV=production webpack --config webpack.user.config.js",
    "dev": "npm run dev:dapp",
    "dev:dapp": "webpack-dev-server --env.STAGING=false --https --content-base public --port 5000",
    "dev:user": "webpack-dev-server --config webpack.user.config.js --https --content-base public --port 5001"
  },
  "devDependencies": {
    "@vue/preload-webpack-plugin": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^6.4.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "^2.1.1",
    "dotenv": "^10.0.0",
    "dotenv-webpack": "^7.0.3",
    "eslint": "^7.27.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^0.6.0",
    "serve": "^11.0.0",
    "style-loader": "^0.23.1",
    "svelte": "^3.43.2",
    "svelte-loader": "2.13.3",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@download/blockies": "^1.0.3",
    "@ensdomains/content-hash": "^2.5.7",
    "@ensdomains/ens-contracts": "^0.0.7",
    "@svicons/ionicons-solid": "^0.1.3",
    "bnc-onboard": "1.35.2",
    "ethers": "^5.4.7",
    "idna-uts46-hx": "^3.4.0",
    "svelte-spa-router": "^3.2.0",
    "web3.storage": "^3.3.2"
  }
}
